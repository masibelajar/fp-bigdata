# .env file for the fp-bigdata project

# --- PostgreSQL Database ---
# This DATABASE_URL must match the user/password in your docker-compose.yaml
DATABASE_URL=postgresql://postgres:strongpassword@localhost:5432/mydatabase

# --- Kafka ---
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# --- MinIO (S3-compatible Object Storage) ---
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin

# --- Spark ---
SPARK_MASTER_URL=spark://localhost:7077